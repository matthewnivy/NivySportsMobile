{
  "name": "UNARoarLions",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clear": "cd android && ./gradlew clean && cd ..",
    "prebuild": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "cd android && ./gradlew assembleDebug && cd ..",
    "build-dev": "cd android && ENVFILE=.env.dev ./gradlew assembleRelease",
    "build-prod": "cd android && ENVFILE=.env.live ./gradlew assembleRelease",
    "build-production": "cd android && ENVFILE=.env.live ./gradlew bundleRelease",
    "test": "jest",
    "lint": "eslint .",
    "ios:s": "npx react-native run-ios --simulator='iPhone 8'",
    "ios:big": "npx react-native run-ios --simulator='iPhone 14 Pro Max'"
  },
  "dependencies": {
    "@react-native-community/checkbox": "^0.5.12",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-navigation/bottom-tabs": "^6.3.3",
