    "@react-navigation/native-stack": "^6.8.0",
    "axios": "^0.27.2",
    "formik": "^2.2.9",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-native": "0.72.15",
    "react-native-blob-util": "^0.19.0",
    "react-native-config": "^1.5.0",
    "react-native-default-preference": "1.4.3",
    "react-native-device-info": "^10.3.0",
