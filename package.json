    "react-native-modal": "^13.0.1",
    "react-native-modal-dropdown": "^1.0.2",
    "react-native-msal": "3.1.1",
    "react-native-onesignal": "^5.2.2",
    "react-native-otp-textinput": "0.0.9",
    "react-native-pdf": "6.5.0",
    "react-native-permissions": "3.6.1",
